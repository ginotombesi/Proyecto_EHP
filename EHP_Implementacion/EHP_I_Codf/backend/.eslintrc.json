{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off",
      "import/no-extraneous-dependencies": "off",
      "func-names": "off",
      "no-underscore-dangle": "off"
    }
  }